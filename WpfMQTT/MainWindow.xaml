<Window x:Class="WpfMQTT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox Name="txtTopicSubscribe"  HorizontalAlignment="Left" Height="23" Margin="42,77,0,0" TextWrapping="Wrap" Text="TopicSubscribe" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="txtReceived"  HorizontalAlignment="Left" Height="23" Margin="42,32,0,0" TextWrapping="Wrap" Text="Received" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="txtTopicPublish"  HorizontalAlignment="Left" Height="23" Margin="42,129,0,0" TextWrapping="Wrap" Text="TopicPublish" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="txtPublish"  HorizontalAlignment="Left" Height="23" Margin="42,173,0,0" TextWrapping="Wrap" Text="Publish" VerticalAlignment="Top" Width="120"/>
        <Button Content="btnSubscribe" HorizontalAlignment="Left" Margin="200,80,0,0" VerticalAlignment="Top" Width="75" Click="btnSubscribe_Click"/>
        <Button Content="btnPublish" HorizontalAlignment="Left" Margin="200,132,0,0" VerticalAlignment="Top" Width="75" Click="btnPublish_Click"/>
    </Grid>
</Window>
